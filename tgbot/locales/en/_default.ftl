user-start_cmd = Hello, user
languages-uk = Ukrainian
languages-en = English
user-language_changed = Your language has been changed to English
random-number =
    { $number ->
        [zero] Zero { $number}
        [one] One { $number}
        [few] Few { $number}
        [many] Many { $number}
        *[other] {$number}
    }


gender-info = 
    { $gender ->
      [male] He is a student
      [female] She is a student
      *[other] They are a student
    }
