languages-uk = Українська
languages-en = Англійська
user-start_cmd = Привіт, користувач! Як я можу допомогти вам?
user-language_changed = Ваша мова була змінена на { languages-uk }
random-number =
    { $number ->
        [zero] Нуль { $number}
        [one] Один { $number}
        [few] Кілька { $number}
        [many] Багато { $number}
        *[other] {$number}
    }

gender-info = 
    { $gender ->
      [male] Він - студент
      [female] Вона - студентка
      *[other] Він/Вона - студент/студентка
    }